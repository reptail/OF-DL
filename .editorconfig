# Editor configuration, see https://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 4
tab_width = 4
end_of_line = crlf
insert_final_newline = true
trim_trailing_whitespace = true

[*.{sln,csproj,xml,json,config}]
indent_size = 2
indent_style = space
tab_width = 2
